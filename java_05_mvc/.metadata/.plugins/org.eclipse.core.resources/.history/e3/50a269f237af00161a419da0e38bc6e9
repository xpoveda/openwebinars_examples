package com.openwebinars.springrest.crud.model;

public class Error {
	
	private String mensaje;
	
	public Error() { }

	public Error(String mensaje) {
		this.mensaje = mensaje;
	}

	public String getMensaje() {
		return mensaje;
	}

	public void setMensaje(String mensaje) {
		this.mensaje = mensaje;
	}
	
	

}
